  },
  "description": "Enhance HTML with custom attributes for clean, server-driven interactivity.",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dependency-cruiser": "^17.0.1",
    "esbuild": "^0.25.8",
    "happy-dom": "^18.0.1",
    "vitest": "^3.2.4"
  },
  "files": [
    "keml.js",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://thealjey.github.io/keml/",
  "keywords": [
    "html",
    "server-rendered",
    "declarative",
    "web framework",
    "no javascript",
    "interactive html",
    "htmx alternative",
    "progressive enhancement",
    "keml"
  ],
  "license": "MIT",
  "main": "keml.js",
  "name": "keml",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thealjey/keml.git"
  },
  "scripts": {
    "build": "node esbuild.config.mts",
    "demo:realworld": "python -m examples.realworld.server",
    "demo:todo": "python -m examples.todo.server",
    "docs:deploy": "mkdocs gh-deploy --force",
    "docs:serve": "mkdocs serve",
    "pip:install": "pip install --upgrade markdown mkdocs mkdocs-material",
    "prebuild": "depcruise 'src/*.mts'",
    "test": "vitest run"
  },
  "version": "3.2.5"
